<h1>YOUR BOOKINGS</h1>



<div class="container">
  <div class="row">
    <% @bookings.each do |booking| %>
      <div class="col-xs-12 col-sm-4">
        <!-- insert <div class="card"> -->
          <div class="card">
            <div class="card-category">
              <%= booking.start_time %> <br>
              <%= booking.end_time %> <br>
              <%= booking.price %> <br>
            </div>
            <div class="card-description">
              <%= link_to 'Details', booking_path(booking) %>
            </div>
          </div>
      </div>
    <% end %>
  </div>
</div>
